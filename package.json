{
  "name": "aileen-orm",
  "version": "0.1.0",
  "description": "TypeOrm Plugin For Aileen Framework",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "build": "rm -rf dist && tsc --build tsconfig.build.json",
    "test": "cross-env NODE_ENV=dev ts-mocha --exit test/**/*.*.ts",
    "cov": "nyc --reporter=lcov npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laohu-aileen/aileen-orm.git"
  },
  "keywords": [
    "TypeOrm",
    "Aileen"
  ],
  "author": "胡永强",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laohu-aileen/aileen-orm/issues"
  },
  "homepage": "https://github.com/laohu-aileen/aileen-orm#readme",
  "devDependencies": {
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@types/power-assert": "^1.5.3",
    "@types/reflect-metadata": "^0.1.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "power-assert": "^1.6.1",
    "prettier": "^2.2.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "aileen-annotation": ">=0.2.0",
    "aileen-core": ">=0.2.0",
    "typeorm": ">=0.2.0"
  }
}